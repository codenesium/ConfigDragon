<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
  </configSections>
  <connectionStrings>
      <add name="testConnectionString" connectionString="YOUR_CONNECTION_STRING_VALUE" />
  </connectionStrings>
  <appSettings>
    <add key="testAppSetting" value="YOUR_APP_SETTING_VALUE" />
    <add key="testAppSettingReplacement" value="123456" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="logfile" xsi:type="File" layout="${longdate} ${callsite} ${message} ${exception:format=tostring}" fileName="${basedir}\ConfigDragon.log" />
      <target xsi:type="console" name="Console" layout="${longdate} ${callsite} ${message} ${exception:format=tostring}" />
    </targets>
    <rules>
        <logger name="*" minlevel="TRACE" writeTo="logfile" />
        <logger name="*" minlevel="TRACE" writeTo="console" />
    </rules>
  </nlog>
</configuration>